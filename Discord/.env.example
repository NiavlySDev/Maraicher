# Variables d'environnement pour le bot Discord Theronis Harvest

# Token du bot Discord (à obtenir depuis https://discord.com/developers/applications)
DISCORD_TOKEN=YOUR_DISCORD_BOT_TOKEN_HERE

# ID du serveur Discord
GUILD_ID=YOUR_GUILD_ID_HERE

# ID du canal pour les logs (optionnel)
LOG_CHANNEL_ID=YOUR_LOG_CHANNEL_ID_HERE

# Configuration de la base de données
DB_PATH=./database/users.db

# Configuration du serveur web (API pour le site)
PORT=3001
API_SECRET=your_super_secret_api_key_here

# Configuration du site web
SITE_URL=http://localhost:3000